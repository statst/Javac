package com.in28minutes.oops;

public class MotoBike {
	// state
	private int speed;

	// behavior
	// method
	// inputs - int speed
	// outputs - void
	// name - setSpeed

	void setSpeed(int speed) { // local variable
		if(speed >0) {
			this.speed = speed;
		}
	}

	// getter method to get speed
	int getSpeed() {
		return speed;
	}
	void start() {
		System.out.println("Bike Started");
	}
}
