package com.in28minutes.oops;

public class MotorBikeRunner {

	public static void main(String[] args) {
		MotoBike ducati = new MotoBike();
		MotoBike honda = new MotoBike();

		ducati.start();

		honda.start();

		// change of state
		ducati.speed = 100;
		honda.speed = 80;

		ducati.speed = 20;
		honda.speed = 10;
	}
}
