package com.in28minutes.oops;

public class MotorBikeRunner {

	public static void main(String[] args) {
		MotoBike ducati = new MotoBike();
		MotoBike honda = new MotoBike();

		ducati.start();

		honda.start();

		// change of state
		ducati.setSpeed(100);
//		System.out.println(ducati.getSpeed());
		honda.setSpeed(80);

		ducati.setSpeed(20);
		honda.setSpeed(10);
	}
}
